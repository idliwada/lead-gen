{
    "manifest_version": 3,
    "name": "Lead Gen",
    "version": "1.0.0",
    "description": "B2B Lead Generation powered by Apify Leads Finder API",
    "permissions": [
        "storage"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}